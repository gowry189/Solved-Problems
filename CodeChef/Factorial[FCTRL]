#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int t,i,k,a;
    long n,ans;
  cin>>t;
  for(i=0;i<t;i++)
  {
      ans=0;
      k=1;
      cin>>n;
      do{
          a=pow(5.0,k);
          ans=ans+(n/a);
          k++;
        }while(a<n);
      cout<<ans<<"\n";
  }
  return 0;
}
 
